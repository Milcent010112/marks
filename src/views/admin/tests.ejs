<div class="page-container">
    <%- include('../partials/sidenav/admin') %>

    <div class="page-container__main page-container__main--nav-closed">
        <%- include('../partials/header/admin') %>

        <div class="page-container__main__padded">
            <div class="page-container__main__padded__page-title">
                <h1>Tests and exams</h1>
            </div>
            <div class="card table">
                <div class="card__header card__header--no-border card__header--search">
                    <input type="text" id="tests-search-box" placeholder="Search tests by lecturer first name or last name, or name or module name or code">
                </div>
                <div class="table__header">
                    <ul class="table__header__row">
                        <li class="table__header__row__item short">#</li>
                        <li class="table__header__row__item">Lecturer</li>
                        <li class="table__header__row__item">Name</li>
                        <li class="table__header__row__item">Module</li>
                        <li class="table__header__row__item">Weight</li>
                        <li class="table__header__row__item last-cell">Added on</li>
                    </ul>
                </div>
                <div class="table__body" id="tests"></div>
                <div class="card__body" id="no-tests" style="display: none;">
                    <div class="empty">
                        <h4>No tests</h4>
                        <p>Tests have not yet been uploaded :)</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script>
    const targetPage = 'admin-tests';
</script>

<script src="/assets/js/dist/lib/jquery/jquery.js"></script>
<script src="/assets/js/dist/app.bundle.js"></script>